def Check_password_Complexity(password):
rating=0
length=len(password)
if(length>=8):
rating+=1
if(length>=12):
rating+=1
if(c.islower() for c in password):
rating+=1
if(c.isupper() for c in password):
rating+=1
if(c.isdigit() for c in password):
rating+=1
if any(c in "!@#$%^&*()-_=+<>/?,,;;'[[\]~`" for c in password):
rating+=1
return rating

password=input("Enter a password ")
rating-Check_password_Complexity(password)
print(f"The pssword complexity Rating is (rating)/10")
